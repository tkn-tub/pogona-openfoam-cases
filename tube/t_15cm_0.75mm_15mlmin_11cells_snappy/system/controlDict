application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.8;

deltaT          0.005;

writeControl    timeStep;

writeInterval   200;

purgeWrite      10;

writeFormat     ascii;

writePrecision  7;

writeCompression off;

timeFormat      general;

timePrecision   6;

// adjustTimeStep  false;

runTimeModifiable true;

